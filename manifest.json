{
	"update_url": "https://clients2.google.com/service/update2/crx",
	"name": "HTML5 media speed controller + subs",
	"description": "Speed controls and subtitles for HTML5 media.",
	"options_page": "options.html",     
	"manifest_version": 3,
	"version": "1.51.2",
	"content_scripts": [
	{
	  "matches": ["<all_urls>"],
	  "css": ["style.css"],
	  "js": ["balancetext.min.js","speedcontrol.js"],
	  "all_frames": true
	}
	],
	"permissions": ["fontSettings","activeTab","storage"],
	"action": {
    "default_icon": "icon.png"
	},
	"icons": {
      "128": "icon.png"
    }
}